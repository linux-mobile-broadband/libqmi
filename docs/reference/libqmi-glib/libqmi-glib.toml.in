[library]
version = "@VERSION@"
description = "A glib-based library for talking to WWAN modems and devices which speak the Qualcomm MSM Interface (QMI) protocol"
license = "LGPL-2.1-or-later"
authors = "The libqmi authors"
browse_url = "https://gitlab.freedesktop.org/mobile-broadband/libqmi.git"
repository_url = "https://gitlab.freedesktop.org/mobile-broadband/libqmi/"
website_url = "https://www.freedesktop.org/wiki/Software/libqmi/"
dependencies = [ "GObject-2.0", "Gio-2.0"]
logo_url = "libqmi.svg"
devhelp = true
search_index = true

  [dependencies."GObject-2.0"]
  name = "GObject"
  description = "The base type system library"
  docs_url = "https://docs.gtk.org/gobject/"

  [dependencies."Gio-2.0"]
  name = "Gio"
  description = "GObject Interfaces and Objects, Networking, IPC, and I/O"
  docs_url = "https://docs.gtk.org/gio/"

[theme]
name = "basic"
show_index_summary = true
show_class_hierarchy = true

[extra]
content_files = [
  @CONTENT_FILES@
]
content_images = [
  "libqmi.svg",
]
urlmap_file = "urlmap.js"

[source-location]
# The base URL for the web UI
base_url = "https://gitlab.freedesktop.org/mobile-broadband/libqmi/-/blob/main/"
# The format for links, using "filename" and "line" for the format
file_format = "{filename}#L{line}"
